<urlrewrite>
     <rule>
        <name>seo redirect</name>
        <condition name="host" operator="notequal">^www.travelopick.com</condition>
        <condition name="host" operator="notequal">^localhost</condition>
        <condition name="host" operator="notequal">^192.168.110.253</condition>
        <from>^/(.*)</from>
        <to type="permanent-redirect" last="true">https://www.travelopick.com/$1</to>
    </rule>
	
	   <rule>
       <from>^/cheap-flights/cheap-flights-to-dubai-dxb</from>
