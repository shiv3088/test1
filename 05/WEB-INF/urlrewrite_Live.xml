<urlrewrite>
<rule match-type="regex">
<condition type="header" operator="equal" name="X-Forwarded-Proto">^HTTP$</condition>
<from>^.*$</from>
<to type="permanent-redirect" last="true">https://www.verticaltrape.com</to>
</rule>	
<rule match-type="regex">
<condition type="header" operator="equal" name="X-Forwarded-Proto">^HTTPS$</condition>
<condition name="host" operator="equal">^verticaltrape.com/*</condition>
<from>^(.*)$</from>
<to type="permanent-redirect" last="true">https://www.verticaltrape.com$1</to>
</rule>


